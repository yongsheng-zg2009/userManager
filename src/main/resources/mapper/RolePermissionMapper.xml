<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cyx.dao.RolePermissionMapper">

    <select id="listAll" resultType="com.cyx.po.RolePermission">
        SELECT 
        	permission_id AS permissionId, 
        	role_id AS roleId
         FROM t_um_role_permission;
    </select>
</mapper>
