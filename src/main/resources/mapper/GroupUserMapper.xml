<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cyx.dao.GroupUserMapper">

    <select id="listAll" resultType="com.cyx.po.GroupUser">
        SELECT 
        	user_name AS userName, 
        	group_id AS groupId
         FROM t_um_group_user;
    </select>
    
</mapper>
